{
  "name": "my_website",
  "version": "1.0.0",
  "description": "My Profile on the internet",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir=dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manmodesanket/my_website.git"
  },
  "author": "Sanket Manmode",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manmodesanket/my_website/issues"
  },
  "homepage": "https://github.com/manmodesanket/my_website#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^4.13.0",
    "onchange": "^6.1.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0"
  }
}
